  "author": "Masud Mirzazada <masudmirzazada@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf dist",
    "build": "tsc",
    "start": "npm ci --force --only=production && node dist/index.js",
    "watch": "tsc -w",
    "dev": "yarn clean && yarn build && concurrently \"yarn watch\" \"nodemon src/index.ts\"",
    "typeorm-cli": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm-cli -- migration:generate --dataSource dist/database/connection.js -n",
    "migration:run": "npm run typeorm-cli -- migration:run --dataSource dist/database/connection.js"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/fastify": "^2.1.1",
    "@fastify/accepts": "^5.0.2",
    "@fastify/cors": "^11.0.1",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-fastify": "^3.13.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "concurrently": "^9.2.0",
    "dataloader": "^2.2.3",
    "fastify": "^5.4.0",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "pino": "^9.7.0",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "type-graphql": "^2.0.0-rc.2",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.25",
    "ulid": "^3.0.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.15",
    "jest": "^30.0.5",
    "pino-pretty": "^13.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
